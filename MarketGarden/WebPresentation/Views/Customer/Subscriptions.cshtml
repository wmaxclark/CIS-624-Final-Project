@model IEnumerable<DataObjects.WeeklyShareViewModel>

@{
    ViewBag.Title = "Subscriptions";
}

<h2>Subscriptions</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Operation.OperationName)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Operation.OperationName)
            </td>
        </tr>
    }
</table>
